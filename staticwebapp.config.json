{
    "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad?post_login_redirect_uri=https://jandan.net/"
    }
  },
    "navigationFallback": {
        "rewrite": "/index.html"
    }
}